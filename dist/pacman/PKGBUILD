pkgname=usenew
pkgver=0.1
pkgrel=1
pkgdesc="wine prefix manager with some libs around it written for POSIX shells and in some parts for bash shell"
arch=('any')
#url="http://nadvsh.sourceforge.net/"
license=('GPL2')
depends=('sh' 'bash' 'coreutils' 'sed' 'wine' 'libsh')
source=("${pkgname}-${pkgver}.tar.gz")
md5sums=('0f186af01092c20a1dd77f53b5a7c71f')

makedepends=('sh-make-tools')



package() {
  cd $srcdir/$pkgname-$pkgver
  sh dist/build_app
  mv pkg/* $pkgdir/.
}